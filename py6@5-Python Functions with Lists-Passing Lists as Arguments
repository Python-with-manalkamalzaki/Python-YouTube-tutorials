#list in a function

#code 1
sum=0
for i in range(1,5):
    sum+=i
print(sum)

#code 2
numbers=[1,2,3,4]
def list(numbers):
   return numbers
print(list(numbers))  #we print the return

#code 3   #multiply the list by 5 and give a new list
numbers=[5,2,3,4]
def change_list(numbers):
    new_numbers=[]
    for i in numbers:
       x=i*5
       new_numbers.append(x)
    print(new_numbers)
change_list(numbers)


